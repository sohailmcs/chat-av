    <script id="viewPatHistory" type="text/html">
 
            <div class="kanban-sidebar" style="overflow: auto;">
              <div class="card shadow-none quill-wrapper">
               
               
                <div class="modal-header bg-primary " style="text-align:center">
                  <h4>Patient History</h4>
                  <button
                    type="button"
                    class="close close-icon"
                    onclick="viewHistory()"
                  >
                    <i class="bx bx-x"></i>
                  </button>       
                 
                </div>
               
                <!-- form start -->
                <div class="modal-body">
                  <div class="row">
                       <div class="col-lg-3">
                         {{#PatientPhoto}}
                         <img
                         src="{{PatientPhoto}}"
                         alt="image"
                         style="
                         height: 100px;
                         width: 100px;
                         border-radius: 50%;" />
                        {{/PatientPhoto}}
                        {{^PatientPhoto}}
                        <img src="/assets/images/patient.png" alt="image" style="
                        height: 100px;
                        width: 100px;
                        border-radius: 50%;" /> 
                        {{/PatientPhoto}}          
                     </div>
                   <div class="col-lg-4">
                     <br/>
                     <h5 class="docName">
                       {{PatientName}}
                     </h5>          
                     <b>Age</b>: {{Age}} years old
                   </div>
                   <div  style="margin-top:10px" class="col-lg-5">
                     <br/>        
                     <b> Visit Date : </b> {{CallLogStartDateTime | Shortdate}} <br>                  
                     <b>Phone </b>: {{PatientPhone}}
                   </div>
             
                 </div>
                     <ul style="padding-left:0;font-size:13px;">
                       <br/>
                       <div class="container profileArea">
                       <div class="row" style="background-color:#F3F8FF;">
                          <br/>
                          <div class="col-sm-5">              
                          <span class="editprofile-icons">History and Exams :</span>
                          </div>
                          <div class="col-sm-7 txtnormal">              
                           {{HistoryAndExam}}
                          </div>
                       </div>
                       
                        
                       <div class="row">
                          <br/>
                          <div class="col-sm-5">             
                          <span class="editprofile-icons">Allergies : </span>
                          </div>
                          <div class="col-sm-7 txtnormal">             
                           {{Allergies}}
                          <br/>
                          </div>
                        </div>
             
                      
                        <div class="row" style="background-color:#F3F8FF;">
                           <br/>
                           <div class="col-sm-5 ">               
                           <span class="editprofile-icons">Diagnosis :</span>
                           </div>
                           <div class="col-sm-7 txtnormal">
                             {{Diagnosis}}             
                          </div>
                        </div>
                
                      <div class="row">             
                         <div class="col-sm-5">             
                         <span class="editprofile-icons">RX :</span>
                        
                         </div>
                         <div class="col-sm-7 txtnormal">             
                           {{PatientRX}}
                           </div>
             
                        </div>
                       </div>
                      
                     </ul>
                   </div>
              </div>
            </div>

</script>


    <!-- BEGIN: Content-->
    <div class="app-content content">
      <div class="content-wrapper">
        <div class="content-body">
          <section
          class="sideMenuId"
            style="
              margin-right: -60%;
              overflow: auto;
              display: none;
              width: 60%;
              margin-top: 30px;
            "
          >
           
          </section>

          <br />
          <div class="d-flex flex-row">
            <div class="page-heading">
              <h3><i class='bx bx-layer'></i><span>Kindah Patients</span></h3>
          </div>
          </div>

        
          <table id="kindahdatatable" class="display" style="width: 100%;">
            <thead class="tablehead">
              <tr>
                <th class="thead-img"></th>
                <th class="call-log-table-head">MRN</th>
                <th class="call-log-table-head">Full Name</th>
                <th class="call-log-table-head">Age</th>
                <th class="call-log-table-head">Gender</th>
                <th class="call-log-table-head">Email</th>
                <th class="call-log-table-head" >Phone Number</th>
              </tr>
            </thead>
          </table>
        </div>
      </div>
