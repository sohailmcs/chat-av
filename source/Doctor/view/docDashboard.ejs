</div>

<script  id="PrescriptionEdit-template" type="text/html">
    <div
    class="modal-dialog modal-dialog-centered modal-dialog-scrollable"
    role="document"
  >
    <div class="modal-content">
      <div class="modal-header bg-primary " style="text-align:center">
        <h4><strong>Patient History</strong></h4>
        <button type="button" class="close"   aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>         
       
      </div>
      <div class="modal-body">
     <div class="row">
          <div class="col-lg-2">
            {{#PatientPhoto}}
            <img
            src="{{PatientPhoto}}"
            alt="image"
            style="
            height: 80px;
            width: 80px;
            border-radius: 10px;
            margin-bottom:10px;"
             />
           {{/PatientPhoto}}
           {{^PatientPhoto}}
           <img src="/assets/images/patient.png" alt="image" style="
           height: 80px;
           width: 80px;
           border-radius: 10px;
           margin-bottom:10px;" /> 
           {{/PatientPhoto}}          
        </div>
      <div class="col-lg-5">
        <br/>
        <h5 class="docName" style="margin:0;">
          {{PatientName}}
        </h5>          
        <b>Age</b>: {{Age}} years old
      </div>
      <div   class="col-lg-5">
        <br/>        
        <b> Visit Date : </b> {{AddedDate | Shortdate}} <br>                  
        <b>Phone </b>: {{PatientPhone}}
      </div>
     </div>
    
     <div class="row">
        <div class="col-sm-12">        
        <span>History and Exams</span>
        <textarea  class="form-control" id="txtExamEdit" rows="2"
            placeholder="Write History and Exams">{{HistoryAndExam}}</textarea>
        </div>
        <div class="col-sm-12">
            <span>Allergies</span>
        <textarea class="form-control"  id="txtAllergiesEdit" rows="2"
            placeholder="Write about Allergies">{{Allergies}}</textarea>
        </div>
        <div class="col-sm-12">
        <span>Diagnosis</span>
        <textarea class="form-control"  id="txtDiagnosisEdit" rows="2"
            placeholder="Write Diagnosis">{{Diagnosis}}</textarea>
        </div>
        <div class="col-sm-12">
        <span>RX</span>
        <textarea class="form-control"  id="txtRxEdit" 
        rows="2" placeholder="Write RX ">{{Diagnosis}}</textarea>
        </div>
     </div>
     <div style="text-align:left;">
        <button style="padding:8px 30px;" cLogId ="{{CallLogID}}" id="btnSaveUpdateEMR" type="button" class="btn btn-primary">
            <i class="bx bx-check d-block d-sm-none"></i>
            <span class="d-none d-sm-block">SAVE</span>
        </button>
       
    </div>
    </div>
  </div>   
  </div>
  </script>

<!-- Scheduled Tempates-->
<script id="Scheduled-template" type="text/html">
    {{#PatientScheduleded}}
    <li class="timeline-items1 timeline-icon-primary active pb-0">
    <div class="col-lg-12">
    <div class="row">
    <div class="col-sm-4">


    <div class="pr-50 pImage " style="float:left;top:30px;">
    <!-- {{#PatientPhoto}}
    <img src="{{PatientPhoto}}" alt="image" style="width: 25px;height: 25px; float:left;"/>
    {{/PatientPhoto}}
    {{^PatientPhoto}}
    <img src="/assets/images/maledoc.png" alt="image" style="width: 25px;height: 25px; float:left;"/>
    {{/PatientPhoto}} -->
    <span class="onlinStatus status_{{PatientID}}" style="margin-right: 5px; !important" ><b>
    {{#OnlineStatus}}
    <span class="bullet bullet-success bullet-sm" ></span>
    {{/OnlineStatus}}
    {{^OnlineStatus}}
    <span class="bullet bullet-offline bullet-sm"></span>
    {{/OnlineStatus}}
    </b>
    </span>
    </div>


    <div class="sh_divqueue" style="display: inline-block;">
    <h6 class='patientName' style="padding-top: 2%;">
    {{patientName}}
    </h6>
    </div>


    <P class="timeline-title-Gender">
    <span class="spnGender"><strong>Gender:</strong> </span> {{Gender}}
    </p>
    </div>
    <div class="col-sm-5">
    <h4 class="timeline-text">
    <strong>Contact: </strong>{{patientPhone | phone}}
    </h4>
    <P class="timeline-text">
    <strong>Time: {{AppointmentStartTime}}-{{AppointmentEndTime}}</strong>
    </p>
    </div>
    <div class="col-sm-3" style="padding: 0 10px;">
    <button type="button" docID="{{DoctorID}}" PName="{{patientName}}"
