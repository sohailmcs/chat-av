<div class="modal fade text-left" id="popupConfirm" tabindex="-1" role="dialog" aria-labelledby="myModalLabel160"
  aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable" role="document">
    <div class="modal-content">
      <div class="modal-header bg-primary ">
        <h4 id="myModalLabel160">
         <strong>Appointment Delete</strong>
        </h4>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>       
      </div>
      <div class="modal-body">
        <h5 class="txtCancellation">Are You Sure You Want to delete This Appointment</h5>
      </div>
      <div class="modal-footer">
        <input type="hidden" id="hdnPatId" value="">
        <input type="hidden" id="hdnAppId" value="">
        <input type="hidden" id="hdnDocId" value="">
        <button type="button" class="btnConfirmDelete btn btn-outline-primary">
          <i class='bx bx-check-square' ></i>
          <b>Yes</b>
        </button>
        <button type="button" data-dismiss="modal" style="padding: 7px 25px;" class="btn btn-primary">
          <i class='bx bxs-x-square' ></i>
         <b>No</b>
        </button>       

      </div>
    </div>
  </div>
</div>

