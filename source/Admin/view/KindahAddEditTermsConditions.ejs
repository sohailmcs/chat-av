    <div class="content-wrapper">
        <div class="content-body">
            <br />
            <div class="d-flex flex-row">
                <div class="page-heading">
                    <h3 id="lblHeading">Create Terms and Conditions</h3>
                </div>
            </div>
            <!-- basic info -->
            <section>
                <div class="row">
                    <div class="col-md-12">
                        <form id="frmTerms" class="form-horizontal">
                            <div class="card">
                                <div class="card-header">
                                    <h4 class="card-title" id="lblRoleHeading"></h4>
                                    <div class="row col-sm-6">
                                        <span class="error" style="display: none;"></span>
                                    </div>
                                </div>
                                <div class="card-content">
                                    <div class="card-body">
                                        <div class="row">
                                            <div class="col-sm-6">
                                                <div class="form-group">                                                  
                                                    <label class="form-label" for="txtKeyValue">Key Value</label>
                                                        <input maxlength="50" type="text" id="txtKeyValue"
                                                         name="name" class="form-input form-control"
                                                             required
                                                            data-validation-required-message="This  field is required" />                                                    
                                                </div>
                                            </div>
                                            <div class="col-sm-12">
                                                <div class="form-group">                                                  
                                                    <label>Terms & Conditions</label>
                                                    <textarea class="form-input form-control" id="txtValueNamEn" rows="3"
                                                    ></textarea>                                                  
                                                </div>
                                            </div>
                                            
                                        <div class="col-sm-4">
                                            <button id="btnSubmit" type="submit" class="btn btn-primary">
                                                Update
                                            </button> &nbsp;
                                            <a href="/Admin/all-values">
                                            <button id="btnCancel" type="button" class="btn btn-primary">
                                                Cancel
                                            </button>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                           
                        </form>
                    </div>
                </div>
            </section>
