                                                                <textarea required class="form-input form-control" id="txtAnswer" rows="3"></textarea>

                                                            </div>
                                                        </div>
                                                    </div>
                                                    <button id="btnSubmit" type="submit" class="btn btn-primary">
                                                        Add Question
                                                    </button> &nbsp;
                                                    <a href="/Admin/all-faq">
                                                        <button id="btnCancel" type="button" class="btn btn-primary">
                                                        Cancel
                                                    </button>
                                                    </a>
                                                </div>

                                            </div>
                                        </div>
                                </div>

                                </form>
                            </div>
