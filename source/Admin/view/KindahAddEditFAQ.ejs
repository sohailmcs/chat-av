                        <!-- basic info -->
                        <section>
                            <div class="row">
                                <div class="col-md-12">
                                    <form id="frmFAQ" class="form-horizontal">
                                        <div class="card createRole">
                                            <div class="card-header">
                                                <h4 class="card-title" id="lblFAQHeading"></h4>
                                                <div class="row col-sm-6">
                                                    <span class="error" style="display: none;"></span>
