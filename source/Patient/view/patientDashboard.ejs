<!--start of doctor Details template--->
<script id="user-template" type="text/html">
  {{#result}}
 <li class=" card-padding mail-read ">

  <div class="row" style="width:100%">
  <!-- picture start -->
      <div class="pr-50 doc-img col-sm-2">
        {{#ProfilePicture}}
           <img src="{{ProfilePicture}}" alt="avtar img holder">                      
        {{/ProfilePicture}}
        {{^ProfilePicture}}
           <img src="/assets/images/maledoc.png" alt="avtar img holder">                      
        {{/ProfilePicture}}                             
      </div>
  <!-- picture start -->

  <!-- description start -->
      <div class="col-sm-4" style="display: inline-block;">
       <h5 class="docName status_{{DoctorId}}">
         Dr. {{FullName}}
          {{#OnlineStatus}} 
          <!-- <span class="bullet bullet-success bullet-sm"></span>           -->
          <span class="docStatus txtDocOnlineStatus">&nbsp;(Available Today)</span>
          {{/OnlineStatus}}

          {{^OnlineStatus}}
          <!-- <span class="bullet bullet-offline bullet-sm"></span>           -->
          <span class="txtDocOffLinneStatus">&nbsp;(Not Available Today)</span>
          {{/OnlineStatus}}
        </h5>
        <span class="ratingArea txtspecialization">
          <input type="hidden" class="SumOfRating" value="{{SumOfRating}}">
          <input type="hidden" class="totalRating" value="{{TotalRating}}">          
          <span style="font-weight:500"> Rating :</span>
           <!-- <i class='bx bxs-star'></i>
           <i class='bx bxs-star'></i>
           <i class='bx bxs-star'></i> -->
      </span>
        
         <div class="modal-primary mr-1   ">                
                <button  style="font-size:0.8rem"type="button" docID="{{DoctorId}}" class="pt-dash-button btndoctorProfile btn btn-outline-primary   btn-sm">
                  <i class="bx bxs-show"></i>
                  <b>View Doctor Profile</b>
                </button>
         </div>         
      </div>
   <!-- description end -->
      <!--- payment area start -->
      <div class="col-sm-3" style="padding-top:1%" >
        <!-- <h5 class="docName status_1">Dr. {{FullName}} Fee </h5> -->
        {{#FeeStructure}}
       <span class="txtspecialization"><strong> Fee</strong> : {{FeeAmount}} SAR / {{CallDuration}}</span>
        <!-- <span class="txtspecialization"><b> {{CallType}} : {{FeeAmount}} SAR / {{CallDuration}}</b></span> -->
        {{/FeeStructure}}
      </div>
      <!--- payment area end -->
    <!-- buttons start -->
          <div class="col-sm-3" >            
              <!-- VIEW DOCTOR PROFILE START -->             
              <div style="text-align:left"  class="call-Queue-icons">               
                <button  fullName="{{FullName}}"  docEmail="{{Email}}" docID="{{DoctorId}}"
                style="font-size:0.8rem" type="button"             
                class="btnAppointments btn btn-outline-primary  mr-1  
                btn-sm btn-block"
                >
                  <i class="bx bx-calendar"></i>
                  <b>Request an Appointment</b>
                </button>             
              {{#OnlineStatus}}
              <a >                              
                <!-- Button trigger for request a call modal -->                              
                <button  type="button" fullName="{{FullName}}"  docEmail="{{Email}}" docID="{{DoctorId}}"
                 class=" btnSencCallReq drcall_{{DoctorId}} btn btn-outline-primary  mr-1  btn-sm btn-block" 
                 style="font-size:0.8rem; margin-top: 3%;">
                  <i class="bx bx-phone-call"></i>
                  <b>Join Sameday Waiting list</b>
                </button>                            
              </a>
              {{/OnlineStatus}}
              {{^OnlineStatus}}
              <a >                              
                <!-- Button trigger for request a call modal -->  
                <button type="button" disabled="disabled" fullName="{{FullName}}"  docEmail="{{Email}}" docID="{{DoctorId}}" 
                 class="btnSencCallReq drcall_{{DoctorId}} btn btn-outline-primary mr-1  btn-sm btn-block" 
                style="font-size:0.8rem; margin-top: 3%;">
                  <i class="bx bx-phone-call"></i>
                  <b>Join Sameday Waiting list</b>
                </button>                            
              </a>
              {{/OnlineStatus}}
              </div>
            </div>
        
    <!-- buttons start -->

          </div>
</li>
  {{/result}}
</script>

<!--end of doctor Details templates---->
<!--start of profile Template-->
<script id="docProfile-template" type="text/html">
      <div
      class="modal-dialog modal-dialog-centered modal-dialog-scrollable"
      role="document"
    >
   
      <div class="modal-content">
       
        <div class="modal-header bg-primary " style="text-align:center">
          <h4>Doctor Profile</h4>
          <button type="button" style="top:5px" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button> 
         
         
        </div>
        <div class="modal-body">
    <div class="row">
          <div class="col-sm-3">    
              {{#ProfilePicture}}
              <img
              src="{{ProfilePicture}}"
              alt="image"
            style="height: 100px; width: 100px; border-radius: 5px;" />
            {{/ProfilePicture}}
            {{^ProfilePicture}}
            <img
              src="/assets/images/maledoc.png"
              alt="image"
              style="height: 100px; width: 100px; border-radius: 5px; margin:0 10px;" />
            {{/ProfilePicture}} 
          </div>
          <div class="col-sm-8">
            <div>
              <br/>
              <h5 class="docName">
                 {{FullName}}
              </h5>
               <span style="font-size:16px"> 
              <b>Specialization</b>: {{Specialization}}
               </span>
            </div>
          </div>
        </div>
          <ul style="padding-left:0;font-size:13px;">
            <br/>
            <br/>

            <div class="container profileArea">
            <div class="row" style="background-color:#F3F8FF;">
               <br/>
               <div class="col-sm-4">              
               <span class="editprofile-icons">Gender </span>
               </div>
               <div class="col-sm-8 txtnormal">              
              {{Gender}}
               </div>
            </div>
            
             
            <div class="row">
               <br/>
               <div class="col-sm-4">             
               <span class="editprofile-icons">Experience  </span>
               </div>
               <div class="col-sm-8 txtnormal">             
                {{#DoctorExperianceModel}}
                <span>{{Designation}}</span>&nbsp;               
               <span>From&nbsp;<strong>{{From}}</strong>&nbsp;to&nbsp;<strong>{{To}}</strong></span>&nbsp;
               at&nbsp;<span>{{HospitalName}}</span><br>
               {{/DoctorExperianceModel}}

               {{^DoctorExperianceModel}}
                <span>&nbsp;</span>
                {{/DoctorExperianceModel}}
               </div>
             </div>

           
             <div class="row" style="background-color:#F3F8FF;">
                <br/>
                <div class="col-sm-4 ">               
                <span class="editprofile-icons">Proficiency </span>
                </div>
                <div class="col-sm-8 txtnormal">                
               <span>{{Proficiency}} </span>
               
               
               </div>
             </div>
     
           <div class="row">             
              <div class="col-sm-4">             
              <span class="editprofile-icons">Career </span>             
              </div>
              <div class="col-sm-8 txtnormal txtBiography">             
               {{{Biography}}}
                </div>

             </div>
            </div>
           
          </ul>
        </div>
       
      </div>
    </div>   
  </script>
<!--end of profile Template-->

<!--start of doctor profile model-->
<div class="modal fade text-left" id="primary" tabindex="-1" role="dialog" aria-labelledby="myModalLabel160"
  aria-hidden="true">

</div>
<!--end of doctor profile model-->

<!--start of call accepted model-->
<div class="modal fade text-left" id="notificationmodal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel160"
  aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable" role="document">
    <div class="modal-content">
      <div class="modal-header bg-primary " style="text-align:center">
        <h4>Call Received</h4>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>                
      </div>
      <br />
      <div class="modal-body" style="text-align: center;">
        <div style="text-align: center;">
          <img src="/assets/Frame 10 (1).png" alt="Calling " />
        </div>
        <br />
        <p><strong>
          Your Token number is <span id="spanTokenNo"></span>
          <br />
          The doctor will be call you after <span id="waitingTime"></span> minutes. As per our policy each patient is provided the 15
          minutes of consultation.</strong>

        </p>
        <br />
      </div>
    </div>
  </div>
</div>
<!--end of call accepted model-->
<!--start of reject call-->
<div class="modal fade text-left" id="dialogRejected" tabindex="-1" role="dialog" aria-labelledby="myModalLabel160"
  aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable" role="document">
    <div class="modal-content">

      <div class="modal-header bg-primary " style="text-align:center">
        <h4>Call rejected</h4>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>         
       
      </div>
      <div class="modal-body" style="text-align: center;">
        <br />

        <h5 class="txtnormal">
          <i>Your call has been rejected by doctor because of some reason.
            Please try again latter
          </i>
        </h5>
        <br />

        <h5 class="txtnormal">
          <i>or you can try to book Appointment with doctor</i>
        </h5>
        <br />
        <br />
      </div>
    </div>
  </div>
</div>
<!--end of reject call -->
    



