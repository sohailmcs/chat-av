            <!-- BEGIN: Content-->

            <div class="app-content content">
                <div class="content-wrapper">
                    <div class="content-body">
                        <br />
                        <div class="d-flex flex-row">
                            <div class="page-heading">
                                <h3>Suggestion Details</h3>
                            </div>
                        </div>
                        <!-- basic info -->
                        <section>
                            <div class="row">
                                <div class="col-md-12">
                                    <form id="frmSuggestion" class="form-horizontal">
                                        <div class="card">

                                            <div class="card-content">
                                                <div class="card-body">
                                                    <div class="row">
                                                        <div class="col-md-6">
                                                            <label>Email</label>

                                                            <input type="email" id="txtEmail" name="txtEmail"
                                                                class="txtInfo form-control " required="required"
                                                                data-validation-required-message="The  field is required" />

                                                        </div>

                                                        <div class="col-sm-6">
                                                            <label>FullName</label>
                                                            <input maxlength="50" type="text" id="txtFullName"
                                                                name="txtFullName" class="txtInfo form-control "
                                                                required="required"
                                                                data-validation-required-message="This  field is required" />

                                                        </div>
                                                    </div>
                                                    <!--end of row-->
                                                    <div class="row mt-2">
                                                        <div class="col-sm-6">
                                                            <div class="row">
                                                                <div class="col-sm-3">
                                                                    <label>Ext</label>
                                                                    <input type="text" disabled="disabled" required
                                                                        id="txtExtension" class="form-control"
                                                                        value="+966" />
                                                                </div>
                                                                <div class="col-sm-9">
                                                                    <label for="txtPhoneNumber">Phone No</label>
                                                                    <input
                                                                        oninput="this.value = this.value.replace(/[^0-9.]/g, ''); 
                                                                    this.value = this.value.replace(/(\..*)\./g, '$1');"
                                                                        required="required" name="contactNo"
                                                                        type="number" class="form-control"
                                                                        id="txtPhoneNumber" />
                                                                </div>

                                                            </div>
                                                        </div>
                                                        <div class="col-sm-6 mt-2">
                                                            <a class='btnUploadSuggestion' href="#">
                                                                AttachFile
                                                                <input type="file" id="imgsuggestions"
                                                                    class="MedicationReciept" name="file_source"
                                                                    size="100">
                                                                <input type="hidden" value="" class="lblAttachment">
                                                            </a>
                                                            &nbsp;
                                                            <span class='label label-info' id="upload-file-info"></span>
                                                        </div>

                                                    </div>
                                                    <!--end of row -->
                                                    <div class="row mt-2">
                                                        <div class="col-sm-6">
                                                            <label for="txtComplain">Complain</label>
                                                            <textarea cols="60" rows="5" id="txtComplain"
                                                                class="form-control" required="required"
                                                                data-validation-required-message="This  field is required"></textarea>

                                                        </div>
                                                    </div>
                                                    <!--end of row -->
                                                    <div class="row">
                                                        <div class="col-sm-12 mt-2">
                                                            <button id="btnCancel" type="submit"
                                                                class="btn btn-primary">
                                                                Send
                                                            </button>
                                                            <br>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>



                                </div>

                                </form>
                            </div>
                    </div>
                    </section>
                </div>
            </div>
            </div>

